    <child link="zed2_camera_center"/>
  </joint>
  <!-- Camera Center -->
  <link name="zed2_camera_center">
    <inertial>
      <mass value="0.16140"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.0003947" ixy="0.0" ixz="3.3365e-8" iyy="2.417e-5" iyz="0.0" izz="0.0003973"/>
    </inertial>
    <visual>
      <geometry>
        <mesh filename="package://ur_description/sensors/zed2/ZED2.dae"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://ur_description/sensors/zed2/ZED2.dae"/>
